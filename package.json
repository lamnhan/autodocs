{
  "name": "@lamnhan/docsuper",
  "version": "0.0.3",
  "description": "Document generator for Typescript projects.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "main": "./src/public-api.js",
  "typings": "./src/public-api.d.ts",
  "bin": {
    "docsuper": "./src/index.js"
  },
  "files": [
    "src",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "lint": "gts check",
    "fix": "gts fix",
    "build": "npm run compile && npm i -g",
    "docs": "docsuper generate && docsuper generate -c docsuper-web.config.js",
    "test": "npm run compile && nyc --cache false mocha test/**/*.js",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "docs",
    "typescript",
    "typedoc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lamnhan/docsuper.git"
  },
  "bugs": {
    "url": "https://github.com/lamnhan/docsuper/issues"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "fs-extra": "^8.1.0",
    "marked": "^0.7.0",
    "prettier": "^1.18.2",
    "tty-table": "^2.8.0",
    "typedoc": "^0.15.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/fs-extra": "^8.0.0",
    "@types/marked": "^0.6.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.12",
    "@types/prettier": "^1.18.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "gts": "^1.1.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
